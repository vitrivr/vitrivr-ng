{
  "api": {
    "host": "default"
  },
  "resources": {
    "host_thumbnails": "http://localhost:4567/thumbnails/:o/:s:x",
    "host_objects": "http://localhost:4567/objects/:n"
  },
  "competition": {
    "host": "dmi-dres.dmi.unibas.ch:443",
    "tls": true,
    "log": false,
    "lsc": false,
    "vbs": false
  },
  "mlt": {
    "VIDEO": [
      "scenecaption",
      "localfeatures",
      "objectInstances"
    ],
    "IMAGE": [
      "scenecaption",
      "localfeatures",
      "objectInstances"
    ]
  },
  "query": {
    "history": 0,
    "options": {
      "image": true,
      "audio": false,
      "model3d": false,
      "motion": false,
      "text": true,
      "tag": true,
      "semantic": true,
      "boolean": true,
      "map": true
    },
    "text": {
      "categories": [
        [
          "ocr",
          "Text on Screen"
        ],
        [
          "asr",
          "Audio"
        ]
      ]
    },
    "boolean": [
      [
        "Example Freetext",
        "TEXT",
        "features_daily.text"
      ],
      [
        "Example Dropdown",
        "OPTIONS",
        "features.option",
        "ONE",
        "TWO",
        "THREE"
      ],
      [
        "Example Range",
        "RANGE",
        "features.range",
        5,
        20
      ],
      [
        "Example Dynamic Options",
        "DYNAMICOPTIONS",
        "features.option",
        "features",
        "option"
      ]
    ],
    "staged": true,
    "enableTagPrioritisation": true
  },
  "refinement": {
    "filters": [
      [
        "dominantcolor.color",
        "CHECKBOX"
      ],
      [
        "technical.duration",
        "SLIDER"
      ]
    ]
  }
}
