{
  "name": "vitrivr-ng",
  "version": "2.5.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startalt": "ng serve --port 5300",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "gen-api": "openapi-generator generate -g typescript-angular -i https://raw.githubusercontent.com/vitrivr/cineast/master/docs/openapi.json -o openapi/cineast --skip-validate-spec --additional-properties npmName=@cineast-openapi/api,snapshot=true,ngVersion=13.0.0",
    "gen-local-api": "openapi-generator generate -g typescript-angular -i http://localhost:4567/openapi-specs -o openapi/cineast --skip-validate-spec --additional-properties npmName=@cineast-openapi/api,snapshot=true,ngVersion=13.0.0",
    "gen-dev-api": "openapi-generator generate -g typescript-angular -i https://raw.githubusercontent.com/vitrivr/cineast/dev/docs/openapi.json -o openapi/cineast --skip-validate-spec --additional-properties npmName=@cineast-openapi/api,snapshot=true,ngVersion=13.0.0",
    "gen-dres-client": "openapi-generator generate -g typescript-angular -i https://raw.githubusercontent.com/dres-dev/DRES/master/doc/oas-client.json -o openapi/dres --skip-validate-spec --additional-properties npmName=@dres-client-openapi/api,ngVersion=13.0.0,enumPropertyNaming=original",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "2.0.3",
    "@angular/animations": "13.0.0",
    "@angular/cdk": "13.0.0",
