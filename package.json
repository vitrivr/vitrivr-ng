    "flatted": "3.2.4",
    "jszip": "3.8.0",
    "leaflet": "1.7.1",
    "leaflet-geosearch": "3.6.0",
    "ngx-color-picker": "11.0.0",
    "ngx-infinite-scroll": "10.0.1",
    "node": "^17.4.0",
    "npm": "^8.11.0",
    "openapi-generator": "^0.1.39",
    "openseadragon": "^2.4.2",
    "rxjs": "7.5.5",
